CSRNonWeighted	make_csr.cpp	/^struct NonWeightCSR CSRNonWeighted(int num_vertices, int num_edges, int directed, ifstream &fin)$/;"	f
CSRWeighted	make_csr.cpp	/^struct WeightCSR CSRWeighted(int num_vertices, int num_edges, int directed, ifstream &fin)$/;"	f
CUDA_OUTPUT	Makefile	/^CUDA_OUTPUT := output$/;"	m
FILENAMES	Makefile	/^FILENAMES := file.txt$/;"	m
INPUT_DIR	Makefile	/^INPUT_DIR := .\/sample_graphs$/;"	m
MAKE_CSR_HPP	make_csr.hpp	2;"	d
NonWeightCSR	make_csr.hpp	/^struct NonWeightCSR$/;"	s
SYCL_OUTPUT	Makefile	/^SYCL_OUTPUT := sycl_output$/;"	m
WeightCSR	make_csr.hpp	/^struct WeightCSR$/;"	s
col_ind	make_csr.hpp	/^    vector<int> col_ind;$/;"	m	struct:NonWeightCSR
col_ind	make_csr.hpp	/^    vector<int> col_ind;$/;"	m	struct:WeightCSR
row_ptr	make_csr.hpp	/^    vector<int> row_ptr;$/;"	m	struct:NonWeightCSR
row_ptr	make_csr.hpp	/^    vector<int> row_ptr;$/;"	m	struct:WeightCSR
weights	make_csr.hpp	/^    vector<int> weights;$/;"	m	struct:WeightCSR
